{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.extras",
      "deno.ns",
      "deno.unstable"
    ],
    "types": [
      "https://deno.land/x/aleph@1.0.0-beta.43/types.d.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/v126/react@18.2.0"
  },
  "importMap": "./import_map.json",
  "tasks": {
    "dev": "deno run -A https://deno.land/x/aleph@1.0.0-beta.44/dev.ts ./server.ts --unstable",
    "start": "deno run -A --unstable server.ts",
    "build": "deno run -A --unstable server.ts --build",
    "esm:add": "deno run -A https://esm.sh/v126 add",
    "esm:update": "deno run -A https://esm.sh/v126 update",
    "esm:remove": "deno run -A https://esm.sh/v126 remove"
  }
}