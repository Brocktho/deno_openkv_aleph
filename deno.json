{
  "lock": false,
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ],
    "alias": {
      "process": "globalThis.process"
    }
  },
  "importMap": ".vscode/resolve_npm_imports.json",
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ --unstable dev.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  }
}
