{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "deployctl deploy --prod --include=build,public --project=final-portfolio ./build/index.js",
    "dev": "remix build && remix dev -c 'deno run --unstable --watch -A ./build/index.js'",
    "format": "deno fmt --ignore=node_modules,build,public/build",
    "lint": "deno lint --ignore=node_modules,build,public/build",
    "start": "cross-env NODE_ENV=production deno run --unstable --allow-net --allow-read --allow-env ./build/index.js",
    "typecheck": "deno check"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.3",
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/deno": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@sentry/remix": "^7.57.0",
    "clsx": "^1.2.1",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.12.18",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-typedjson": "^0.1.7",
    "tiny-invariant": "^1.3.1",
    "ulid": "^2.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.18.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
